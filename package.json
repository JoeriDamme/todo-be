{
  "name": "example-code-joeri-damme",
  "version": "1.2.0",
  "description": "Example code by Joeri Damme",
  "main": "dist/start.js",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "dev": "NODE_CONFIG_ENV=dev NODE_ENV=production ts-node-dev --respawn --poll -r tsconfig-paths/register src/server.ts",
    "build": "NODE_ENV=production rimraf ./dist && NODE_ENV=production tsc",
    "start": "NODE_ENV=production npm run build && NODE_ENV=production node dist/start.js",
    "test": "NODE_CONFIG_ENV=test mocha -r ts-node/register -r tsconfig-paths/register './test/**/*.spec.ts'",
    "test:integration": "NODE_CONFIG_ENV=test mocha -r ts-node/register -r tsconfig-paths/register --file integration-test/helper.ts './integration-test/**/*.spec.ts'"
  },
  "keywords": [
    "Example",
    "Code",
    "Test",
    "Joeri",
    "Damme"
  ],
  "author": "Joeri Damme (joerid@gmail.com)",
  "license": "ISC",
  "_moduleAliases": {
    "@controllers": "dist/controllers",
    "@helpers": "dist/helpers",
    "@lib": "dist/lib",
    "@middlewares": "dist/middlewares",
    "@routes": "dist/routes",
    "@services": "dist/services",
    "@models": "dist/models",
    "@interfaces": "dist/interfaces"
  },
  "pre-commit": [
    "npm run lint",
    "npm test"
  ],
  "devDependencies": {
    "@types/bluebird": "^3.5.33",
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.14",
    "@types/config": "0.0.38",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.167",
    "@types/mocha": "^8.2.0",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.20",
    "@types/sinon": "^9.0.10",
    "@types/supertest": "^2.0.10",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uniqid": "^5.2.0",
    "@types/validator": "^13.1.3",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "chai": "^4.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.17.0",
    "mocha": "^8.2.1",
    "rimraf": "^3.0.2",
    "sinon": "^9.2.3",
    "supertest": "^6.1.3",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "config": "^3.3.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-http-context": "^1.2.4",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "mysql2": "^2.2.5",
    "node-cache": "^5.1.2",
    "sequelize": "^6.5.0",
    "sqlite3": "^5.0.2",
    "swagger-ui-express": "^4.1.6",
    "uniqid": "^5.2.0",
    "winston": "^3.3.3",
    "yamljs": "^0.3.0"
  }
}
